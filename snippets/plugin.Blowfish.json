{
  "${BlowFish_Decrypt}": {
    "body": "blowfish::decrypt \\$${1:return_variable} \"${2:text_to_decrypt}\" \"${3:password_key}\"$4",
    "prefix": "BlowFish_Decrypt"
  },
  "${BlowFish_Encrypt}": {
    "body": "blowfish::encrypt \\$${1:return_variable} \"${2:text_to_encrypt}\" \"${3:password_key}\"$4",
    "prefix": "BlowFish_Encrypt"
  }
}
