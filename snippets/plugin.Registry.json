{
  "${Registry::Close}": {
    "body": "${Registry::Close} \"${1:handle}\"$2",
    "prefix": "${Registry::Close}"
  },
  "${Registry::CopyKey}": {
    "body": "${Registry::CopyKey} \"${1:full_path_source}\" \"${2:full_path_target\new_key_name}\" $${3:var}$4",
    "prefix": "${Registry::CopyKey}"
  },
  "${Registry::CopyValue}": {
    "body": "${Registry::CopyValue} \"${1:full_path_source}\" \"${2:value_source}\" \"${3:full_path_target}\" \"${4:value_source}\" $${5:var}$6",
    "prefix": "${Registry::CopyValue}"
  },
  "${Registry::CreateKey}": {
    "body": "${Registry::CreateKey} \"${1:full_path}\" $${2:var}$3",
    "prefix": "${Registry::CreateKey}"
  },
  "${Registry::DeleteKeyEmpty}": {
    "body": "${Registry::DeleteKeyEmpty} \"${1:full_path}\" $${2:var}$3",
    "prefix": "${Registry::DeleteKeyEmpty}"
  },
  "${Registry::DeleteKey}": {
    "body": "${Registry::DeleteKey} \"${1:full_path}\" $${2:var}$3",
    "prefix": "${Registry::DeleteKey}"
  },
  "${Registry::DeleteValue}": {
    "body": "${Registry::DeleteValue} \"${1:full_path}\" \"${2:value}\" $${3:var}$4",
    "prefix": "${Registry::DeleteValue}"
  },
  "${Registry::Find}": {
    "body": "${Registry::Find} \"${1:handle}\" \"${2:options}\" $${3:path} $${4:value_or_key} $${5:string} $${6:type}$7",
    "prefix": "${Registry::Find}"
  },
  "${Registry::HexToStr}": {
    "body": "${Registry::HexToStr} \"${1:string}\" $${2:var}$4",
    "prefix": "${Registry::HexToStr}"
  },
  "${Registry::KeyExists}": {
    "body": "${Registry::KeyExists} \"${1:full_path}\" $${2:var}$3",
    "prefix": "${Registry::KeyExists}"
  },
  "${Registry::MoveKey}": {
    "body": "${Registry::MoveKey} \"${1:full_path_source}\" \"${2:full_path_target\new_key_name}\" $${3:var}$4",
    "prefix": "${Registry::MoveKey}"
  },
  "${Registry::MoveValue}": {
    "body": "${Registry::MoveValue} \"${1:full_path_source}\" \"${2:value_source}\" \"${3:full_path_target}\" \"${4:value_source}\" $${5:var}$6",
    "prefix": "${Registry::MoveValue}"
  },
  "${Registry::Open}": {
    "body": "${Registry::Open} \"${1:full_path}\" \"${2:options}\" $${3:var}$4",
    "prefix": "${Registry::Open}"
  },
  "${Registry::ReadExtra}": {
    "body": "${Registry::ReadExtra} \"${1:full_path}\" \"${2:value}\" \"${3:number}\" $${4:value_read} $${5:type_read}$6",
    "prefix": "${Registry::ReadExtra}"
  },
  "${Registry::Read}": {
    "body": "${Registry::Read} \"${1:full_path}\" \"${2:value}\" $${3:value_read} $${4:type_read}$5",
    "prefix": "${Registry::Read}"
  },
  "${Registry::RestoreKey}": {
    "body": "${Registry::RestoreKey} \"${1:file}\" $${2:var}$3",
    "prefix": "${Registry::RestoreKey}"
  },
  "${Registry::SaveKey}": {
    "body": "${Registry::SaveKey} \"${1:full_path}\" \"${2:file}\" \"${3:options}\" $${4:var}$5",
    "prefix": "${Registry::SaveKey}"
  },
  "${Registry::StrToHex}": {
    "body": "${Registry::StrToHex} \"${1:string}\" $${2:var}$3",
    "prefix": "${Registry::StrToHex}"
  },
  "${Registry::Unload}": {
    "body": "${Registry::Unload}$1",
    "prefix": "${Registry::Unload}"
  },
  "${Registry::WriteExtra}": {
    "body": "${Registry::WriteExtra} \"${1:full_path}\" \"${2:value}\" \"${3:string}\" $${4:var}$5",
    "prefix": "${Registry::WriteExtra}"
  },
  "${Registry::Write}": {
    "body": "${Registry::Write} \"${1:full_path}\" \"${2:value}\" \"${3:string}\" \"${4:type}\" $${5:var}$6",
    "prefix": "${Registry::Write}"
  }
}
