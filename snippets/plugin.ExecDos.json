{
  "ExecDos::exec": {
    "body": "ExecDos::exec \"${1:application_to_run}.exe\" \"${2:stdin_string}\" \"${3:log_file_name}.log\"$4",
    "prefix": "ExecDos::exec"
  },
  "ExecDos::exec [/options]": {
    "body": "ExecDos::exec ${1:options} \"${2:application_to_run}.exe\" \"${3:stdin_string}\" \"${4:log_file_name}.log\"$5",
    "prefix": "ExecDos::exec [/options]"
  },
  "ExecDos::isdone": {
    "body": "ExecDos::isdone /NOUNLOAD ${1:handle}$2",
    "prefix": "ExecDos::isdone"
  },
  "ExecDos::wait": {
    "body": "ExecDos::wait ${1:handle}$2",
    "prefix": "ExecDos::wait"
  }
}
