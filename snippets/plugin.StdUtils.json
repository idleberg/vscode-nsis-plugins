    "prefix": "${StdUtils.ExecShellAsUser}"
  },
  "${StdUtils.ExecShellWaitEx}": {
    "body": "${StdUtils.ExecShellWaitEx} $${1:output1} $${2:output2} \"${3:file}\" ${4:verb} \"${5:args}\"$6",
